{
  "expo": {
    "name": "afrostyles",
    "slug": "afrostyles",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/logo-color.png",
    "scheme": "afrostyles",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-color.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-image-picker"],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*", "assets/fonts/*"]
  }
}
